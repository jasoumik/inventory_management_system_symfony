<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset('tools/select2/select2.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset('tools/datepicker/css/bootstrap-datepicker.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset('tools/agGrid/styles/ag-grid.min.css') }}">
        <link rel="stylesheet" href="{{ asset('tools/agGrid/styles/ag-theme-alpine.min.css') }}">
    {% endblock %}


</head>
<style>
    .table {
        color: black;
    }
</style>
<body>

<div class="container">
    {% block navbar %}
        {{ include('navbar.html.twig') }}
    {% endblock %}

    {% block body %}
    {% endblock %}
</div>

{% block javascripts %}
    <script src="{{ asset('js/jquery-3.2.1.slim.min.js') }}"></script>
    <script src="{{ asset('js/jquery-v3.5.1.min.js') }}"></script>
    <script src="{{ asset('js/jquery.validate.min.js') }}"></script>
    <script src="{{ asset('js/popper.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('tools/select2/select2.min.js') }}"></script>
    <script src="{{ asset('tools/datepicker/js/bootstrap-datepicker.min.js') }}"></script>
    <script src="{{ asset('tools/sweetalert2/sweetalert2@10.js') }}"></script>
    <script src="{{ asset('tools/agGrid/ag-grid-community.min.noStyle.js') }}"></script>

{% endblock %}
</body>
</html>
